{
    "nixEnvSelector.nixShellConfig": "${workspaceRoot}/shell.nix",
    "files.associations": {
        "*.hs": "haskell",
    },
    "[haskell]": {
        "editor.defaultFormatter": "MaxGabriel.brittany"
    },
    "cSpell.words": [
        "Attoparsec",
        "Hspec",
        "delim",
        "mbls",
        "metavar",
        "subparser"
    ]
}